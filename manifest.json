{
  "name": "SHS VPN",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Unblock Discord",
  "homepage_url": "http://example.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "management",
    "proxy"
  ],
  "popup": "src/popup/popup.html"
}
